<script>
	$(document).ready(function ($) {

		$('#khuvuc').modal({
			backdrop: 'static',
			keyboard: false
		})
		//Set cookie
		function setCookie(cname, cvalue, exdays) {

			var date = new Date();
			var minutes = 1;
			date.setTime(date.getTime() + (7*24*60*60*1000));
			var expires = "; expires="+date.toGMTString();
			document.cookie = cname + "=" + cvalue + expires + ";path=/";

		}
		//Get cookie
		function getCookie(cname) {
			var name = cname + "=";
			var decodedCookie = decodeURIComponent(document.cookie);
			var ca = decodedCookie.split(';');
			for(var i = 0; i <ca.length; i++) {
				var c = ca[i];
				while (c.charAt(0) == ' ') {
					c = c.substring(1);
				}
				if (c.indexOf(name) == 0) {
					return c.substring(name.length, c.length);
				}
			}
			return "";
		}

		var checklocal = getCookie("location");	

		$('.wrap-modal-kv ul li').click(function(e){
			var lc = $(this).attr('data-place');
			setCookie("location",lc,1);
			$('#khuvuc').removeClass('in').modal('hide');
			location.reload();
		});

		if(checklocal === 'ha-noi'){
			$('.location_header span').html('Hà Nội');
		}else if (checklocal === 'tp-ho-chi-minh') {
			$('.location_header span').html('TP HCM');
		}else if (checklocal === 'khu-vuc-khac') {
			$('.location_header span').html('KV khác');
		}else if (checklocal === '') {
			$('.location_header span').html('Tỉnh thành');
		}


		if (checklocal == ''){
			$('#khuvuc').addClass('in').modal('show');
		}else {
			$('.modal-backdrop').remove();
		}

		$('.location_header').click(function(e){
			$('#khuvuc').addClass('in khuvuc_on open_opacity').modal('show');
			$('.wrap-modal-kv').addClass('block');
		});
		$('.location_header').click(function(event){
			event.stopPropagation();
			$('#khuvuc').modal('show');
		});
	});

</script>
