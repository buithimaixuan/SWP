	function scrollbar(){
		const postDetails = document.querySelector(".tab_h .content_extab ");
		const postSidebar = document.querySelector(".block-right-module");
		const postSidebarContent = document.querySelector(".block-right-module");

		const controller = new ScrollMagic.Controller();
		const scene = new ScrollMagic.Scene({
			triggerElement: postSidebar,
			triggerHook: 0,
			duration: getDuration
		}).addTo(controller);

		if (window.matchMedia("(min-width: 1200px)").matches) {
			console.log(postDetails.offsetHeight);
			scene.setPin(postSidebar, {pushFollowers: false});
		}

		// in your projects, you might want to debounce resize event for better performance
		window.addEventListener("resize", () => {
			if (window.matchMedia("(min-width: 1200px)").matches) {
				scene.setPin(postSidebar, {pushFollowers: false});
			} else {
				scene.removePin(postSidebar, true);
			}
		});

		function getDuration() {

			return postDetails.offsetHeight - postSidebarContent.offsetHeight;

		}
	}




